//绑定tab切换事件
function bindPanelChange(){
	$('.nav-tabs a').click(function (e) {
	  e.preventDefault();
	  $(this).tab('show');
	  var thisDom = $(this);
	  var idx = thisDom.attr('aria-controls');
	  $('#'+idx).find('.cm-table').datagrid('resize');
	});
}

