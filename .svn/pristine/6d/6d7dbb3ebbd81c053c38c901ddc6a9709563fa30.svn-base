@charset "utf-8";
/*定义变量*/
.layui-layer-dialog .layui-layer-title,
.layui-layer-ext .layui-layer-title {
  background-color: #ffffff;
  background: #ffffff;
  color: #333333;
  border: none;
  padding: 0 80px 0 10px;
  border-radius: 0;
  border-bottom: 1px solid #eeeeee;
}
.layui-layer-ext .layui-layer-btn .layui-layer-btn1,
.layui-layer-dialog .layui-layer-btn a {
  height: 28px;
  line-height: 28px;
  margin: 0 6px;
  padding: 0 15px;
  border: 1px solid #dedede;
  background-color: #f1f1f1;
  color: #333333;
  font-weight: 400;
  cursor: pointer;
  text-decoration: none;
  display: inline-block;
  vertical-align: top;
  border-radius: 0;
}
.layui-layer-dialog .layui-layer-btn .layui-layer-btn0,
.layui-layer-ext .layui-layer-btn a {
  background: #e9e9e9;
  border: none;
  border-radius: 0;
  color: #ffffff;
}
.layui-layer-ext .layui-layer-setwin .layui-layer-close1 {
  background-position: 1px -40px;
}
.layui-layer-title > i.iconfont {
  color: #e9e9e9;
}
.layui-layer-page .layui-layer-content {
  padding: 8px;
}
/* tips层 */
.layui-layer-tips .layui-layer-content {
  background-color: #e9e9e9;
}
.layui-layer-tips .layui-layer-content {
  top: -7px;
}
body .layui-layer-ext-msg {
  border-radius: 0;
  border: 1px solid #dedede;
}
.layui-layer-setwin .layui-layer-ico {
  background: url(../img/layer/icon.png) no-repeat;
}
.layui-layer-ext .layui-layer-btn {
  text-align: center;
}
