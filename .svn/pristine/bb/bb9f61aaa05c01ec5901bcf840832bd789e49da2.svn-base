}
.cm-clearfix:after {
  display: block;
  content: '';
  clear: both;
}
.cm-fl {
  float: left;
}
.cm-fr {
  float: right;
}
.cm-forbid {
  -moz-user-select: none;
  -khtml-user-select: none;
  user-select: none;
}
.cm-form {
  /* 仅适用于面板头部的搜索区 */
}
.cm-form-title {
  width: 100%;
  height: 34px;
  border-bottom: 2px solid #eee;
  margin: 0 0 20px 0;
}
.cm-form-title span {
  position: relative;
  display: inline-block;
  height: 100%;
  font-size: 14px;
  line-height: 34px;
}
.cm-form-title span:before {
  content: '';
  width: 100%;
  height: 2px;
  background-color: #e15b52;
  position: absolute;
  bottom: -2px;
}
.cm-form-readonly {
  height: 26px;
  background-color: #eeeeee;
  border: 1px solid #cccccc;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
  padding: 3px 8px 3px 8px;
  box-shadow: none;
}
.cm-form-area {
  width: 420px;
  height: 90px;
  color: #333333;
  background-color: #ffffff;
  border: 1px solid #cccccc;
  padding: 3px 8px 3px 8px;
  resize: none;
}
.cm-form-editDiv {
  display: inline-block;
  width: 420px;
  min-height: 90px;
  color: #333333;
  padding: 3px 8px 3px 8px;
  background-color: #ffffff;
  border: 1px solid #cccccc;
  outline: none;
}
.cm-form-wrap {
  width: 100%;
}
.cm-form-wrap > li {
  margin: 0 0 10px 0;
}
.cm-form-wrap > li > label {
  width: 105px;
  text-align: right;
  padding: 0 5px 3px 8px;
  margin: 3px 0 0 0;
  vertical-align: top;
}
.cm-form-wrap > li > div {
  display: inline-block;
}
.cm-sign {
  color: #e15b52;
  position: relative;
  top: 2px;
}
.cm-select-wrap {
  width: 158px;
  position: relative;
  border: 1px solid #cccccc;
}
.cm-select-wrap .cm-form-select {
  width: 100%;
  height: 26px;
  background-color: #ffffff;
  color: #333333;
  border: none;
  cursor: pointer;
  padding: 3px 24px 3px 8px;
}
.cm-select-btn {
  color: #aaaaaa;
  position: absolute;
  right: 6px;
  top: 50%;
  cursor: pointer;
  margin: -9px 0 0 0;
  transition: 0.4s all ease;
}
.cm-selectBox-Wrap {
  display: none;
  position: absolute;
  left: -1px;
  top: 27px;
  width: 158px;
  max-height: 200px;
  background-color: #ffffff;
  border: 1px solid #cccccc;
  border-top: none;
  z-index: 9;
}
.cm-selectBox-Wrap > li {
  height: 30px;
  line-height: 30px;
  padding: 0 8px;
  cursor: pointer;
}
.cm-selectBox-Wrap > li:hover {
  background-color: #fce2e2;
  color: #d43f3a;
}
.cm-menu-content {
  display: none;
  position: absolute;
  left: -1px !important;
  top: 26px !important;
  max-height: 300px;
  border: 1px solid #cccccc;
  background: #fff;
  z-index: 9;
}
.cm-dialog-wrap {
  padding: 15px 20px 20px;
  display: none;
}
.cm-redBtn {
  background-color: #e15b52;
  color: #ffffff;
  padding: 4px 6px 4px 25px;
  height: 26px;
}
.cm-redBtn:hover,
.cm-redBtn:focus,
.cm-redBtn:active {
  background-color: #c73c37;
}
.cm-btn-i {
  padding: 3px 8px 3px 8px;
}
.cm-btn-i > i {
  margin: 0 4px 0 0;
  font-size: 13px;
}
.cm-searchWrap {
  float: left;
  padding: 0 0 0 15px;
  margin: 0 0 10px 0;
}
.cm-tips {
  font-size: 20px;
  color: #e15b52;
  cursor: pointer;
}
.cm-tips:hover {
  color: #c73c37;
}
/*================================下面是定义插件的样式修改====================================*/
/***重写bootstrap的样式*/
.container-fluid {
  margin-top: 10px;
}
.panel-default {
  border-color: #fff;
}
.panel-heading {
  background-color: #ffffff;
  border-bottom-color: #ddd;
}
.panel {
  border-radius: 0;
  height: 100%;
}
.panel-body {
  min-height: 300px;
}
.form-inline .form-control {
  display: inline-block;
  height: 26px;
  color: #333333;
  padding: 3px 8px 3px 8px;
  background-color: #ffffff;
  border: 1px solid #cccccc;
  border-radius: 0;
  box-shadow: none;
}
.form-control:focus,
.cm-form-area:focus,
.cm-form-editDiv:focus {
  border-color: #d43f3a;
  background-color: #fffdfd;
  box-shadow: none;
}
.form-inline .input-medium {
  width: 200px;
}
.form-inline .input-small {
  width: 50px;
}
.form-inline > .form-group {
  display: inline-block;
  margin-bottom: 10px;
  vertical-align: middle;
  margin-right: 20px;
}
/* webuploader插件上传文件 */
.picker {
  margin-right: 10px;
}
.webuploader-container .webuploader-pick {
  background-color: #e15b52;
  color: #fff;
  padding: 4px 6px;
  height: 26px;
  border-radius: 0;
}
.webuploader-pick .iconfont {
  margin-right: 3px;
}
/* layer弹出层 */
body .layui-layer-tips > .layui-layer-content {
  color: #716f6d;
}
.layui-layer-setwin .layui-layer-close1 {
  background-position: 1px -40px;
}
/* datatables样式改造 */
.dataTables_wrapper .dataTable {
  border-collapse: collapse;
  border-spacing: 0;
  table-layout: fixed;
}
.dataTables_wrapper .dataTable tr {
  border: 1px solid #eceff8;
  height: 40px;
}
.dataTables_wrapper .dataTable th,
.dataTables_wrapper .dataTable td {
  padding-left: 16px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.dataTables_wrapper .dataTable thead th,
.dataTables_wrapper .dataTable thead td {
  background-color: #f2f2f2;
  border-bottom: none;
  font-weight: normal;
  border-right: 1px solid #fff;
}
.dataTables_wrapper .dataTable th:last-child,
.dataTables_wrapper .dataTable td:last-child {
  border-right: none;
}
table.dataTable tbody tr > th,
table.dataTable tbody tr > td {
  border-right: 1px solid #fff;
}
table.dataTable tbody tr:nth-child(even) {
  background-color: #f9f9f9;
}
table.dataTable tbody tr:nth-child(even) > th,
table.dataTable tbody tr:nth-child(even) > td {
  border-right: 1px solid #f9f9f9;
}
.dataTables_wrapper .dataTable tbody > tr:hover {
  background: #f5f5f5;
}
.dataTables_wrapper .dataTable tbody > tr:hover > th,
.dataTables_wrapper .dataTable tbody > tr:hover > td {
  border-right-color: #fff;
}
table.dataTable td > i.iconfont,
table.dataTable td > a > i.iconfont {
  color: #434343;
}
table.dataTable td > a > i.icon-auth {
  font-size: 12px;
  position: relative;
  top: -2px;
}
.dataTable tr > td > a > i.iconfont:hover,
.dataTable tr > td > a > i.iconfont:active,
.dataTable tr > td > a > i.iconfont:focus {
  color: #d9534f;
}
/* 分页 */
.dataTables_wrapper .dataTables_paginate {
  margin: 20px 0;
}
.panel-body .dataTables_wrapper .dataTables_paginate .paginate_button {
  background: #f3f2f3;
  border-radius: 0;
  padding: 0.2em 0.7em;
}
.dataTables_wrapper .dataTables_paginate .paginate_button:hover,
.dataTables_wrapper .dataTables_paginate .paginate_button.current,
.dataTables_wrapper .dataTables_paginate .paginate_button.current:hover {
  background: #ed5b56;
  border-color: transparent;
  color: #fff !important;
  box-shadow: none;
}
.dataTables_wrapper .dataTables_paginate .paginate_button.disabled,
.dataTables_wrapper .dataTables_paginate .paginate_button.disabled:hover,
.dataTables_wrapper .dataTables_paginate .paginate_button.disabled:active {
  opacity: 0.6;
  filter: alpha(opacity=60);
  background: #f3f2f3;
}
/*ztree树的右键样式*/
.dropdown-menu > li > a > i.glyphicon {
  color: #f39800;
  padding-right: 3px;
}
.dropdown-menu > li > a {
  padding: 3px 16px 3px 12px;
}
.dropdown-menu {
  min-width: 112px;
}
