.cm-clearfix:after {
  display: block;
  content: '';
  clear: both;
}
.cm-fl {
  float: left;
}
.cm-fr {
  float: right;
}
.cm-form {
  /* 仅适用于面板头部的搜索区 */
}
.cm-form-title {
  width: 100%;
  height: 34px;
  border-bottom: 2px solid #eee;
  margin: 0 0 20px 0;
}
.cm-form-title span {
  position: relative;
  display: inline-block;
  height: 100%;
  font-size: 14px;
  line-height: 34px;
}
.cm-form-title span:before {
  content: '';
  width: 100%;
  height: 2px;
  background-color: #e15b52;
  position: absolute;
  bottom: -2px;
}
.cm-form-readonly {
  height: 26px;
  background-color: #eeeeee;
  border: #cccccc 1px solid;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
  padding: 3px 8px 3px 8px;
  box-shadow: none;
}
.cm-form-area {
  width: 420px;
  height: 90px;
  color: #333333;
  background-color: #ffffff;
  border: #cccccc 1px solid;
  padding: 3px 8px 3px 8px;
  resize: none;
}
.cm-form-editDiv {
  display: inline-block;
  width: 420px;
  min-height: 90px;
  color: #333333;
  padding: 3px 8px 3px 8px;
  background-color: #ffffff;
  border: #cccccc 1px solid;
  outline: none;
}
.cm-form-wrap {
  width: 100%;
}
.cm-form-wrap > li {
  margin: 0 0 10px 0;
}
.cm-form-wrap > li > label {
  width: 105px;
  text-align: right;
  padding: 0 5px 3px 8px;
  margin: 3px 0 0 0;
  vertical-align: top;
}
.cm-form-wrap > li > div {
  display: inline-block;
}
.cm-sign {
  color: #e15b52;
  position: relative;
  top: 2px;
}
.cm-select-wrap {
  width: 158px;
  position: relative;
  border: #cccccc 1px solid;
}
.cm-select-wrap .cm-form-select {
  width: 100%;
  height: 26px;
  background-color: #ffffff;
  color: #333333;
  border: none;
  cursor: pointer;
  padding: 3px 24px 3px 8px;
}
.cm-select-btn {
  color: #aaaaaa;
  position: absolute;
  right: 6px;
  top: 50%;
  cursor: pointer;
  margin: -9px 0 0 0;
  transition: 0.4s all ease;
  -webkit-touch-callout: none;
  /* iOS Safari */
  -webkit-user-select: none;
  /* Chrome/Safari/Opera */
  -khtml-user-select: none;
  /* Konqueror */
  -moz-user-select: none;
  /* Firefox */
  -ms-user-select: none;
  /* Internet Explorer/Edge */
  user-select: none;
  /* Non-prefixed version, currently not supported by any browser */
}
.cm-selectBox-Wrap {
  display: none;
  position: absolute;
  left: -1px;
  top: 27px;
  width: 158px;
  max-height: 200px;
  background-color: #ffffff;
  border: #cccccc 1px solid;
  border-top: none;
  z-index: 9;
}
.cm-selectBox-Wrap > li {
  height: 30px;
  line-height: 30px;
  padding: 0 8px;
  cursor: pointer;
}
.cm-selectBox-Wrap > li:hover {
  background-color: #fce2e2;
  color: #d43f3a;
}
.cm-menu-content {
  display: none;
  position: absolute;
  left: -1px !important;
  top: 26px !important;
  max-height: 300px;
  border: #cccccc 1px solid;
  background-color: #ffffff;
  z-index: 9;
}
.cm-dialog-wrap {
  padding: 15px 20px 20px;
  display: none;
}
.cm-redBtn {
  background-color: #e15b52;
  color: #ffffff;
  padding: 4px 6px 4px 25px;
  height: 26px;
}
.cm-redBtn:hover,
.cm-redBtn:focus,
.cm-redBtn:active {
  background-color: #c73c37;
}
.cm-btn-i {
  padding: 3px 8px 3px 8px;
}
.cm-btn-i > i {
  margin: 0 4px 0 0;
  font-size: 13px;
}
.cm-searchWrap {
  float: left;
  padding: 0 0 0 15px;
  margin: 0 0 10px 0;
}
.cm-tips {
  font-size: 20px;
  color: #e15b52;
  cursor: pointer;
}
.cm-tips:hover {
  color: #c73c37;
}
/*================================下面是定义插件的样式修改====================================*/
/***重写bootstrap的样式*/
.container-fluid {
  margin: 10px 0 0 0;
}
.panel {
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
  height: 100%;
}
.panel-default {
  border-color: #fff;
}
.panel-heading {
  background-color: #ffffff;
  border-bottom-color: #ddd;
}
.panel-body {
  min-height: 300px;
}
.form-inline .form-control {
  display: inline-block;
  height: 26px;
  color: #333333;
  padding: 3px 8px 3px 8px;
  background-color: #ffffff;
  border: #cccccc 1px solid;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
  box-shadow: none;
}
.form-inline .input-medium {
  width: 200px;
}
.form-inline .input-small {
  width: 50px;
}
.form-inline > .form-group {
  display: inline-block;
  vertical-align: middle;
  margin: 0 20px 10px 0;
}
.form-control:focus,
.cm-form-area:focus,
.cm-form-editDiv:focus {
  border-color: #d43f3a;
  background-color: #fffdfd;
  box-shadow: none;
}
/* webuploader插件上传文件 */
.picker {
  margin: 0 10px 0 0;
}
.webuploader-container .webuploader-pick {
  background-color: #e15b52;
  color: #ffffff;
  padding: 4px 6px 4px 6px;
  height: 26px;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  -ms-border-radius: 0;
  -o-border-radius: 0;
  border-radius: 0;
}
.webuploader-container .webuploader-pick .iconfont {
  margin: 0 3px 0 0;
}
/*ztree树的右键样式*/
.dropdown-menu {
  min-width: 112px;
}
.dropdown-menu > li > a {
  padding: 3px 16px 3px 12px;
}
.dropdown-menu .iconfont {
  color: #ff6e07;
  padding: 0 3px 3px 8px;
}
