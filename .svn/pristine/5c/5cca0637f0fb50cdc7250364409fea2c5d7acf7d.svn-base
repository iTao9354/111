#tbl1 {
  width: 100%;
  height: 300px;
}
#chart1 {
  width: 100%;
  height: 330px;
}
#tbl2 {
  height: 400px;
}
.excl {
  color: #6eb57b;
  font-size: 20px;
  font-weight: normal;
  cursor: pointer;
}
.edit-icon {
  height: 18px;
  vertical-align: baseline;
  cursor: pointer;
  margin: 1px 5px 0;
}
.col-icon {
  height: 16px;
  vertical-align: baseline;
  cursor: pointer;
}
.fullScreen-icon {
  height: 15px;
  vertical-align: baseline;
  cursor: pointer;
  position: relative;
  top: 3px;
  margin-left: 5px;
}
.close-icon {
  color: #ff6d5e;
  font-size: 14px;
  cursor: pointer;
  margin-left: 5px;
}
.table-btn .excl {
  position: relative;
  top: 3px;
}
