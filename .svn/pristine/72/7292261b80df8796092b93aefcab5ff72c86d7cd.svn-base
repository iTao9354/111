//美化滚动条
function addNiceScroll(){
	$('body').niceScroll({cursorcolor:"#ddd",cursorwidth: "5px",cursorborder:"none"});
}

//绑定tab切换事件
function bindPanelChange(){
	$('.nav-tabs a').click(function(){
		var thisDom = $(this);
		var idx = thisDom.attr('aria-controls');
		$('#'+idx).siblings().removeClass('active').addClass('active');
		if(idx == "colRowTable"){
			initBasicTbl2();
		}else if(idx == "childrenTable"){
			initBasicTbl3();
		}else if(idx == "fixHeaderCol"){
			initBasicTbl45();
		}
	});
}

$(function(){
	initBasicTbl1();
	addNiceScroll();
	bindPanelChange();
});
