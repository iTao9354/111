@charset "UTF-8";
//logo、二三级菜单文字颜色
@mainFontColor: #333;
//头部背景颜色
@frameHeadBkColor: #fff;
//头部右侧带下拉的背景颜色
@dropdownBkColor: #fbf6f6;
//头部右侧带下拉菜单分割线颜色
@dropdownSplitColor: #ffdcdc;
//头部右侧字体图标颜色
@dropdownIconColor: #747272;
//头部右侧字体图标hover颜色
@dropdownIconHoverColor: #111;
//dropdownTipsBk
@dropdownTipsBkColor: #fff;
//下拉块字体颜色
@dropdownTipsFontColor: #333;
@dropdownTipsBoxShadowColor: #ddd;
@dropdownTipsBorderColor: #eee;
//下拉ul li 的hover状态背景颜色
@dropdownBTipsLiHoverBkColor: #f1f1f1;
@leftBarActBkColor: #e2e2e2;
//登录人数提示
@lableClsColor: #fff;
//menu和frame之间分割线颜色、一级菜单左右箭头颜色, tab 激活状态背景颜色
@menuFrameSplitArrowColor: #ff6d5e;
//一级菜单左右箭头颜色，灰色
@menuArrowGrayColor: #eee;
//一级菜单文字颜色
@firstMenuFontColor: #383838;
//一级菜单激活状态背景颜色
@frameHeadActBkColor: #ebeef2;
//一级菜单激活状态下边框颜色，四级菜单hover上去颜色
@frameHeadActBorderColor: #ed5b5a;
//左侧菜单标题和菜单之间分割线颜色
@leftBarIconSplitColor: #ffbf25;
//左侧控制展开收起按钮背景颜色
@leftBarCtrlBkColor: #fff;
//左侧菜单背景颜色
@leftBarBkColor: #f6f6f6;
//左侧菜单激活块背景颜色
@leftBarActBkColor: #e2e2e2;
//左侧菜单激活状态边框颜色和图标颜色
@leftBarActBorderColor: #e15b52;
//二级菜单有子菜单的箭头颜色
@hasChildMenuArrowColor: #b0adad;
//三级和四级菜单之间分割线的颜色
@thirdForthSplitColor: #cbc9c9;
//四级菜单文字颜色
@fourthMenuFontColor: #666666;
//二级菜单背景颜色
@submenuBkColor: #fbfbfb;
@submenuBoxShadowColor:#999;
//tab区域景颜色
@tabWrapBkColor: #fafafa;
@tabBkColor: #d2d2d2;
@tabFontColor: #fff;
@tabActFontColor: #fff;
@redColor: #E15B52;
@blueColor: #2C5B9D;
//全屏以后字体颜色
@fullScrOptFontColor: #fff;
@fullScrBkColor: #343434;
@fullSrcActBkColor:#494949;

html {
    height: 100%;
}

body {
    min-width: 900px;
    height: 100%;
}

a:hover {
    text-decoration: none;
}
*{
    color:@mainFontColor;
}
.frame-container {
    height: 100%;
    overflow: hidden;
    position: relative;
}

.frame-head {
    position: relative;
    height: 50px;
    background: @frameHeadBkColor;
    border-bottom: 1px solid @menuFrameSplitArrowColor;
}

.frame-head h1 {
    width: 320px;
    float: left;
    height: 50px;
    margin: 0;
    padding: 0 0 0 15px;
    position: relative;
    z-index: 4;
    line-height: 50px;
    background: @frameHeadBkColor;
}

.frame-head>h1>span {
    font-size: 18px;
    color: @mainFontColor;
}

.frame-head>h1>img {
    position: relative;
    top: 11px;
}

#nav {
    float: left;
    @val: 532px;
    width: -moz-calc(~"100% - @{val}");
    width: -webkit-calc(~"100% - @{val}");
    width: calc(~"100% - @{val}");
    height: 51px;
    -moz-user-select: none;
    -webkit-user-select: none;
}

.inform {
    width: 18px;
}

.inform>a {
    display: block;
    position: relative;
}

.userRelated {
    overflow: visible;
    width: 220px;
    position: absolute;
    right: 0;
    height: 50px;
}

#user span {
    font-size: 14px;
    width: 64px;
    overflow: hidden;
    white-space: nowrap;
    top: 2px;
    display: inline-block;
    text-overflow: ellipsis;
    position: relative;
}

.userRelated>li {
    display: block;
    float: left;
    line-height: 24px;
    margin: 13px 0;
    padding: 0 13px;
    position: relative;
    overflow: visible;
    border-right: 1px solid @dropdownSplitColor;
}
.userRelated>li:last-child {
    border: none;
}
.userRelated .iconfont {
    font-size: 16px;
}

.userRelated li.li-first {
    float: left;
    padding: 0 13px;
    height: 50px;
    line-height: 50px;
}

.userRelated p>.label {
    border-radius: 10px;
    width: 18px;
    display: inline-block;
    height: 16px;
    text-align: center;
    font-size: 12px;
    padding: 0;
    position: absolute;
    top: -7px;
    left: 6px;
    line-height: 16px;
    background: transparent url(../../../dist/img/main/redCir1.png) no-repeat center top;
}

.systemSetting {
    padding: 0px !important;
}

.dropdownTips {
    position: absolute;
    z-index: 999;
    top: 300px;
    background: @dropdownTipsBkColor;
    line-height: 34px;
    width: 195px;
    right: 50%;
    margin-right: -18px;
    top: 38px;
    color: @dropdownTipsFontColor;
    border:1px solid @dropdownTipsBorderColor;
    border-top-width: 0;
    box-shadow: @dropdownTipsBoxShadowColor 1px 1px 3px;
}

.model-setting {
    display: inline-block;
    width: 16px;
    height: 16px;
    background: url(../../../dist/img/main/setting.png) no-repeat;
    background-size: cover;
    margin-right: 5px;
    line-height: 30px;
    vertical-align: middle;
}

#onlineCountDropDown {
    width: 230px;
}

#noticeDetail {
    right: 5px;
}

.tipsScrollDiv {
    overflow-x: hidden;
}

.dropdownTips-list>li:hover,
.dropdownTips-list>li.active {
    background: @dropdownBTipsLiHoverBkColor;
}
.dropdownTips-list>li.dropdown-model:hover,
.dropdownTips-list>li.dropdown-model.active {
    background: transparent;
}
.sub-model i {
    display: inline-block;
    width: 10px;
    height: 10px;
    border-radius: 5px;
    margin-right: 5px;
    line-height: 20px;
}

i.red {
    background-color: @redColor;
}

i.blue {
    background-color: @blueColor;
}

.dropdownTips-list>li>a {
    color: @dropdownIconColor;
    display: block;
    clear: both;
    font-weight: 300;
    white-space: normal;
    font-size: 13px;
    padding: 0 20px 1px 15px;
    text-shadow: none;
    text-align: left;
}
#userDropDown .dropdownTips-list>li>a{
    padding: 0 8px;
}
#userDropDown{
    padding: 0;
}
.label {
    text-shadow: none!important;
    font-size: 14px;
    font-weight: 300;
    padding: 3px 6px;
    color: @lableClsColor;
    border-radius: 0;
}

.userRelated>li>a,
.userRelated>li>p {
    display: block;
    line-height: 24px;
    height: 24px;
    position: relative;
    cursor: pointer;
}

.head-setting {
    padding-right: 20px;
    padding-top: 12px;
}

.nav>li>a.userInfo>.iconfont {
    font-size: 12px;
}

#userInfoDialog {
    padding-top: 5px;
    overflow: hidden;
}

#errorInfo {
    color: red;
    float: right;
}

.frame-content {
    overflow: hidden;
    position: absolute;
    left: 15.7%;
    right: 0;
    bottom: 0;
    top: 51px;
}

.frame-content.footer {
    bottom: 30px;
}


/*针对easyui的tab样式修改*/

.tabs li.tabs-first>.tabs-close {
    display: none;
}

.tabs-header,
.tabs-tool {
    height: 34px;
    background-color: transparent;
    padding: 0;
}

.tabs {
    height: 26px;
    margin-top: 4px;
}

.tabs li a.tabs-inner {
    top: -2px;
    position: relative;
}

.tabs li.tabs-selected a.tabs-inner,
.tabs li a.tabs-inner,
.tabs li.tabs-selected a.tabs-inner:hover,
.tabs li a.tabs-inner:hover,
.tabs-p-tool a:hover,
.tabs li a.tabs-close:hover {
    background: transparent;
}

.tabs-header,
.tabs-scroller-left,
.tabs-scroller-right,
.tabs-tool,
.tabs,
.tabs-panels,
.tabs li a.tabs-inner,
.tabs li.tabs-selected a.tabs-inner,
.tabs-header-bottom .tabs li.tabs-selected a.tabs-inner,
.tabs-header-left .tabs li.tabs-selected a.tabs-inner,
.tabs-header-right .tabs li.tabs-selected a.tabs-inner {
    border: none;
}

.tabs-header-noborder {
    background-color: @tabWrapBkColor;
    height: 30px;
}

.tabs li.tabs-last,
.tabs li.tabs-selected.tabs-last {
    border-right: 1px solid @tabBkColor;
}

.r-nav-list a {
    display: block;
    width: 100%;
    height: 32px;
    padding: 0 11px 2px 15px;
    display: table-cell;
    vertical-align: middle;
    height: 34px;
}

.r-nav-list a:hover,
.r-nav-list a:focus {
    text-decoration: none;
}

.tabs-title {
    position: relative;
    color: @tabFontColor;
    font-size: 12px;
}

.add-track .tabs li .tabs-title {
    display: inline-block;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.tabs-selected .tabs-title {
    color: @tabActFontColor;
    font-weight: normal;
}

.r-nav-list.active a>span {
    color: @tabActFontColor;
}

.tabs .icon-refresh,
.tabs .tabs-p-tool a.icon-refresh:hover {
    background: transparent url("../../../dist/img/main/refresh.png") no-repeat scroll center center;
    height: 25px;
    zoom: 1;
    position: relative;
    width: 18px;
}

.tabs-first .tabs-p-tool {
    float: right;
    padding-right: 3px;
    position: relative;
    right: 8px;
}

.tabs li.tabs-first a.tabs-inner {
    padding-right: 1px;
}

.tabs-p-tool {
    right: 25px;
}

.tabs li a.tabs-close,
.tabs-p-tool {
    height: auto;
    top: 4px;
}

.tabs li a.tabs-close {
    background: transparent url(../../../dist/img/main/closeTab.png) no-repeat center center;
    width: 18px;
    height: 12px;
    display: inline-block;
    top: 0px;
    position: relative;
}

.tabs li a.tabs-close:hover {
    background: transparent url(../../../dist/img/main/closeTab.png) no-repeat center center;
}

.tabs-scroller-left,
.tabs-scroller-right {
    width: 18px;
    height: 21px;
    border: none;
}

.tabs-scroller-left {
    background: transparent url('../../../dist/img/main/frameCtl.png') no-repeat;
    background-position: 0 4px;
}

.tabs-scroller-right {
    background: transparent url('../../../dist/img/main/frameCtl.png') no-repeat;
    background-position: 0 -20px;
}

.tabs-scroller-left:hover {
    background: transparent url('../../../dist/img/main/frameCtl.png') no-repeat;
    background-position: -20px 4px;
}

.tabs-scroller-right:hover {
    background: transparent url('../../../dist/img/main/frameCtl.png') no-repeat;
    background-position: -20px -20px;
}

.tabs li a.tabs-inner {
    filter: none;
}

.add-track .tabs li a.tabs-inner {
    display: inline-block;
    width: 94px;
    text-align: left;
    padding-top: 9px;
}

.tabs li.tabs-selected a.tabs-inner {
    filter: none;
}

.tabs li {
    border-left: 1px solid @tabWrapBkColor;
    height: 20px;
    margin: 0 3px;
    background-color: #c1c3c3;
    border-radius: 2px;
}

body.treeNav .frame-content {
    left: 0;
}

@media screen and (max-width: 1400px) {
    .frame-content {
        left: 185px;
    }
    body.treeNav .frame-content {
        left: 0;
    }
}


/* 头部菜单样式 */

.dropdownTips {
    display: none;
    border: 0;
}

.dropdownTips-list font {
    font-size: 13px;
}

.dropdownTips-list span {
    font-size: 13px;
}

#userDropDown {
    display: none;
    width: 135px;
    left: -5px;
    clear: both;
}

#userDropDown li {
    height: 35px;
}

.dropdownTips:after {
    left: auto;
    right: 15px;
    border-bottom: 10px solid @dropdownTipsBkColor;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    top: -9px;
    content: "";
    display: inline-block;
    position: absolute;
}
/*修改密码弹窗样式*/
#updatePasswd .form-group{
    overflow: hidden;
}
#updatePasswd .form-group label {
    width: 90px;
    text-align: right;
    display: block;
    float: left;
    top: 10px;
    position: relative;
}

#updatePasswd .input-sm {
    width: 180px;
    line-height: 25px;
    height: 25px;
    float: right;
    margin-right: 36px;
}

#updatePasswd .cSign {
    padding: 0 2px;
}

.layui-layer-page.layui-layer-ext .layui-layer-btn {
    padding-top: 0;
}

.form-group {
    margin-bottom: 15px;
    margin-right: 0px;
    width: 100%;
}

#level {
    float: none;
    margin-left: 90px;
    display: block;
}


/* 我的信息 */

#userInfoDialog {
    padding-top: 5px;
}

#userInfoDialog strong,
#userInfoDialog p {
    font-size: 12px;
    margin: 0 0 5px;
}


/* 全屏切换 */

.noElement {
    display: none;
}

#fsb {
    position: absolute;
    left: 50%;
    bottom: 5em;
    margin-left: -230.5px;
    height: 60px;
    width: 461px;
    z-index: 2;
}

.full-screen-btn {
    float: left;
    visibility: hidden;
    z-index: 2002;
    cursor: pointer;
    background-color: @fullScrBkColor;
}
.full-screen-btn:hover, .full-screen-btn.active {
    background-color: @fullSrcActBkColor;
}
.collapseFsb,
.collapseFsb:hover {
    background: transparent url(../../../dist/img/main/collapsebk.png) no-repeat center right;
    width: 53px;
    color: @fullScrOptFontColor;
    line-height: 60px;
    text-align: center;
    position: relative;
    top: -1px;
}

.collapseFsb .iconfont {
    left: -3px;
    position: relative;
    color: @fullScrOptFontColor;
}

.collapseFsb>span {
    width: 25px;
    display: inline-block;
    top: 1px;
    position: relative;
    color: @fullScrOptFontColor;
}
.fsbordMenu span,.fsbordMenu .iconfont{
    color: @fullScrOptFontColor;
    font-size: 12px;
}
.fsbordMenu .iconfont{
    font-size: 10px;
}
.fsbordMenu {
    background: transparent url(../../../dist/img/main/menubk.png) no-repeat left top;
    width: 60px;
    height: 34px;
    line-height: 34px;
    color: @fullScrOptFontColor;
    position: fixed;
    z-index: 2;
    padding-left: 15px;
    bottom: 20px;
    left: 10px;
    cursor: pointer;
    display: none;
}

.fsbordMenu.active {
    display: block;
}

.showStyle {
    background-color: #343434;
}

.tabs-header-noborder.add-track{
   height: auto;
    position: absolute;
    bottom: 128px;
    left: 50%;
    margin-left: 115px;
    z-index: 40;
    display: none;
}
.add-track .tabs-wrap {
    position: relative;
    left: 0;
    overflow: hidden;
    width: 200px;
    margin: 0;
    padding: 0;
}
.add-track .tabs {
    list-style-type: none;
    height: auto;
    margin: 0px;
    padding: 0px;
    padding-left: 4px;
    width:100%;
    border-style: none;
    border-width: 0 0 1px 0;
}
.add-track .tabs li {
    border-left: 1px solid #eee;
    height: 35px;
    line-height: 35px;
    margin: 0 3px;
    background: rgba(0,0,0,0.8);
//  filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#C8000000,endColorstr=#C8000000);
    background-color: #000;
    opacity: .8;
    filter:alpha(opacity=80);
    float: none;
    border-radius: 0;
    border-bottom: 1px solid #4e4e4e;
    display: block;
    width: 139px;
}
.add-track .tabs li:hover {
    background: #ff9600;
    filter:none;
}
.add-track .tabs li.tabs-first{
    border-radius: 4px 4px 0 0;
}
.add-track .tabs-first .tabs-p-tool {
    padding-right: 0;
}
.add-track .tabs li.tabs-last{
    border-bottom: none;
    border-radius: 0 0 4px 4px;
    border-right: none;
}
.add-track .tabs li.tabs-selected{
    background-color: #ff9600;
    filter: none;
}
.add-track .tabs li .tabs-p-tool {
    top: 12px;
    left: 100px;
}
.add-track .tabs li .tabs-close {
    top: -9px;
    left: 3px;
}
.add-track .tabs li.tabs-first .tabs-p-tool, .add-track .tabs-first .tabs-inner {
    float: left;
    left: 0;
}
.btclose {
    padding: 19px 23px 18px 23px;
}

.mark_left {
    padding: 20px 24px;
}

.pause {
    padding: 19px 25px;
}

.mark_right {
    padding: 20px 24px;
}

.reload {
    padding: 19px 23px;
}

.track {
    padding: 18px 22px;
    position: relative;
}

.track .triangle {
    position: absolute;
    top: -13px;
    font-size: 16px;
    color: #000;
    opacity: 0.8;
    filter: alpha(opacity=80);
}

.track .badge {
    position: absolute;
    right: 14px;
    top: 9px;
    background-color: #d84444;
    cursor: pointer;
}

.badge {
    display: inline-block;
    min-width: 10px;
    padding: 3px 7px;
    font-size: 12px;
    font-weight: 700;
    line-height: 1;
    color: #fff;
    text-align: center;
    white-space: nowrap;
    vertical-align: baseline;
    background-color: #777;
    border-radius: 10px;
    visibility: hidden;
    opacity: 0.5;
    filter: alpha(opacity=50);
}

.yesShow {
    visibility: visible;
}

.full-screen-btn .imgHover {
    opacity: 1;
