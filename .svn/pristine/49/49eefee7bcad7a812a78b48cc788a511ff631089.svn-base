* {
  margin: 0;
  padding: 0;
  font-family: PingFangSC-Light, 'helvetica neue', 'hiragino sans gb', tahoma, 'microsoft yahei ui', 'microsoft yahei', simsun, sans-serif;
  font-size: 12px;
}
body {
  background-color: #e8ecf0;
  color: #333333;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: normal;
  margin: 0;
  padding: 0;
}
h2 {
  font-size: 16px;
}
h3 {
  font-size: 14px;
}
ul,
ol,
li,
dl,
dt,
dd {
  margin: 0;
  padding: 0;
}
/* 有时不生效，需要再次清除margin和padding */
li,
dt,
dd {
  list-style: none;
}
a {
  color: #333333;
  text-decoration: none;
}
a:focus {
  outline: none;
  color: #e15b52;
}
a:link,
a:visited {
  text-decoration: none;
}
a:hover {
  text-decoration: none;
  color: #e15b52;
}
a:active {
  text-decoration: none;
}
/* IE下点击a标签去掉虚线边框 */
img {
  vertical-align: top;
}
input,
button,
textarea {
  outline: none;
}
button {
  border: none;
  outline: none;
}
label {
  font-weight: normal;
}
i {
  font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 14px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
